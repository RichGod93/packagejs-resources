                  "keywords": {
                    "kSType": "S",
                    "kRFormat": "",
                    "kRFormatFlags": "",
                    "kSBytes": "512",
                    "kSNormalization": "",
                    "kSLowerCase": "0",
                    "kSRequired": "0"
                  },
                  "slug": {
                    "kSType": "S",
                    "kRFormat": "[a-z0-9]+(((\\-)|(\\_))[a-z0-9]+)*",
                    "kRFormatFlags": "",
                    "kSBytes": "128",
                    "kSNormalization": "NFD",
                    "kSLowerCase": "1",
