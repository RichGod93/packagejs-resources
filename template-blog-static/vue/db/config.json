        "kSBytes": "",
        "kSNormalization": "",
        "kSLowerCase": "0",
        "kSRequired": "1",
        "kSDefault": ""
      },
      "DEF_DRIVE_PUBLIC_URL": {
        "kSType": "S",
        "kRFormat": "",
        "kSBytes": "",
        "kSNormalization": "",
        "kSLowerCase": "0",
        "kSRequired": "1",
        "kSDefault": ""
      }
    }
  },
  "options": {
    "kSType": "J",
    "kSRequired": "1",
    "kJKeys": {}
  },
  "storage-remote": {
    "prjbucketp": {
      "deployments": {
        "default": {
          "url": "{{ DEF_SERVICES_URL_FOR_STORAGE_PRJBUCKETP }}"
        }
      }
    },
    "prjbuckets": {
      "deployments": {
        "default": {
          "url": "{{ DEF_SERVICES_URL_FOR_STORAGE_PRJBUCKETS }}"
        }
      }
    }
  },
  "drives-remote": {
    "prjdrivep": {
      "deployments": {
        "default": {
          "url": "{{ DEF_SERVICES_URL_FOR_DRIVES_PRJDRIVEP }}",
          "drive-secret": "{{ DEF_SECRET_FOR_PRJDRIVEP }}",
          "drive-secure-read": "0"
        }
      }
    },
    "prjdrives": {
      "deployments": {
        "default": {
          "url": "{{ DEF_SERVICES_URL_FOR_DRIVES_PRJDRIVES }}",
          "drive-secret": "{{ DEF_SECRET_FOR_PRJDRIVES }}",
          "drive-secure-read": "1"
