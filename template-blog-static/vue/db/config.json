      },
      "DEF_DEBUGGING_MODE": {
        "kSType": "S",
        "kRFormat": "^(0|1)$",
        "kSBytes": "1",
        "kSNormalization": "",
        "kSLowerCase": "0",
        "kSRequired": "1",
        "kSDefault": "0"
