          "$aHCoreTablesRenamePrefix": {
            "posts": "625292da5927a64612da75f138a2089aa86c3b46442de30484c0c9ba298f44f4"
          }
        },
        "$aHBuilderConstructorArguments": {},
        "$aHDeployerConstructorArguments": {
          "$aSGenerateRandomHTTPAuth": "1",
          "$aSSSLKeyPEM": "{{ DEF_SERVER_SSL_KEY }}",
          "$aSSSLCertPEM": "{{ DEF_SERVER_SSL_CERT }}"
        },
        "$aHListenerConstructorArguments": {
